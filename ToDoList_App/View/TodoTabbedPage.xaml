<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage
    x:Class="ToDoList_App.TodoTabbedPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:android="clr-namespace:Microsoft.Maui.Controls.PlatformConfiguration.AndroidSpecific;assembly=Microsoft.Maui.Controls"
    xmlns:local="clr-namespace:ToDoList_App"
    Title="TodoTabbedPage"
    android:TabbedPage.ToolbarPlacement="Bottom">
    <NavigationPage Title="Started">
        <x:Arguments>
            <local:TodoMainPage Title="Started" />
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="OnGoing">
        <x:Arguments>
            <local:OnGoingTasksPage Title="OnGoing" />
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="Completed">
        <x:Arguments>
            <local:TodoMainPage Title="Completed" />
        </x:Arguments>
    </NavigationPage>
</TabbedPage>